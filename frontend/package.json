{
  "name": "quizly-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@jridgewell/sourcemap-codec": "^1.5.4",
    "axios": "^1.12.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "sourcemap-codec": "^1.4.8"
  },
  "scripts": {
    "start": "craco start",
    "test": "craco test --env=jsdom --detectOpenHandles --watchAll",
    "test:once": "craco test --env=jsdom --watchAll=false --testPathPattern=tests/frontend",
    "test:ci": "craco test --env=jsdom --watchAll=false --coverage=false",
    "build": "craco build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.23.0",
    "@craco/craco": "^7.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "babel-jest": "^29.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest-environment-jsdom": "^29.7.0"
  },
  "overrides": {
    "glob": "^10.4.5",
    "jsdom": "^20.0.3",
    "svgo": "^3.3.2",
    "webpack-dev-server": "^5.2.1"
  }
}
